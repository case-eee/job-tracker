<% @jobs.each do |job| %>
  <ul><%= link_to job.title, company_job_path(job.company, job) %> at <%= link_to job.company.name, company_path(job.company) %></br>
  <%= link_to job.city, "/companies/#{@company}/jobs?location=#{job.city}" %></br>
  Level of Interest: <%= job.level_of_interest %></br>
    <%= link_to "Edit", edit_company_job_path(job.company, job) %>
    <%= link_to "Delete", company_job_path(job.company, job), method: "delete" %></ul>
    </br></br>
<% end %>