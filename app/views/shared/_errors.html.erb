<div class="container">
  <ul>
    <% @errors.each do |error| %>
      <div class="alert alert-danger" role="alert">
        <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
        <span class="sr-only">Error:</span>
        <%= "Whoops! #{error}." %>
      </div>
    <% end %>
  </ul>
</div>