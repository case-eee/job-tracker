<h1><%= @category.title %></h1>
<%= link_to "Edit", edit_category_path(@category) %>
<%= link_to "Delete", category_path(@category), method: "delete" %>
<h3> jobs in <%= @category.title %> </h3>
<% @category.jobs.each do |job| %>
  <%= link_to "#{job.title} @ #{job.company.name}", company_job_path(job.company.id, job.id) %>
  <br>
<% end %>