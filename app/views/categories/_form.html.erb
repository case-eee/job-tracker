<%= form_for @category, :html => {:class => "form-inline"} do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <%= f.submit class: 'btn btn-default' %>
<% end %>
<button type="button" class= "btn btn-default"><%= link_to "Create a new category", new_category_path %></button>
