<div class = "buttonForm companyForm" >
  <%= form_for @company do |f| %>
    <%= render partial: "shared/errors", locals: { target: @company } %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :city %>
    <%= f.text_field :city %> <br/>

    <%= f.submit %>
  <% end %>
</div>
