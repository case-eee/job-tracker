<%= form_for [@company, @contact] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :position %>
  <%= f.text_field :position %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :notes %>
  <%= f.text_field :notes %>

  <% f.label :company_id %>
  <% f.text_field :company_id %>

  <%= f.submit %>
<% end %>
